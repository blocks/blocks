{
  "private": true,
  "name": "blocks",
  "scripts": {
    "start": "yarn docs",
    "build": "gatsby build",
    "clean": "gatsby clean",
    "docs": "gatsby develop",
    "docs-deploy": "now switch blocks-ui && now && now alias $(pbpaste) blocks-ui.com",
    "prepare": "lerna run prepare",
    "publish": "lerna publish --force-publish=\"*\"",
    "lint:eslint": "eslint ."
  },
  "workspaces": [
    "packages/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/core": "10.0.22",
    "@mdx-js/mdx": "1.5.1",
    "@mdx-js/react": "1.5.1",
    "@theme-ui/components": "0.2.50",
    "@theme-ui/presets": "0.2.44",
    "@theme-ui/prism": "0.2.50",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-flowtype": "^4.5.2",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "gatsby-plugin-catch-links": "2.1.19",
    "gatsby-plugin-compile-es6-packages": "2.1.0",
    "gatsby-plugin-fathom": "1.1.0",
    "gatsby-plugin-manifest": "2.2.31",
    "gatsby-plugin-mdx": "1.0.59",
    "gatsby-plugin-react-helmet": "3.1.16",
    "gatsby-plugin-theme-ui": "0.2.43",
    "gatsby-source-filesystem": "2.1.40",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-helmet": "5.2.1",
    "theme-ui": "0.2.49"
  },
  "devDependencies": {
    "gatsby": "2.18.6",
    "husky": "3.1.0",
    "lerna": "3.19.0",
    "lint-staged": "9.5.0",
    "prettier": "1.19.1"
  }
}
